<!DOCTYPE html>
<html>
  <head><title>Book Suggestions</title></head>
  <body>
    <%= form_for @nn_book_suggestion, url: {action: "create"}, do |f| %>
      <%= f.text_field :editorial %>
      <%= f.text_field :price %>
      <%= f.text_field :author %>
      <%= f.text_field :title %>
      <%= f.text_field :link %>
      <%= f.text_field :publisher %>
      <%= f.text_field :year %>
    <% end %>
  </body>
</html>
